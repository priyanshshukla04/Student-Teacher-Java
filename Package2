void NameOfFaculty()
{
System.out.println("Enter the name of faculty");
String name=fac.nextLine();
private Salary()
{
System.out.println("Enter the salary of the faculty");
salary=fac.nextInt();
}
}
}


public class Student
{
int rollno,
int sapid;
String name;
String branch;
int num1;
int num2;
void Entrance()
{
System.out.println("enter the marks in entrance paper1");
num1=obj.nextInt();
System.out.println("enter the marks in entrance paper2");
num2=obj.nextInt();
try{
System.out.println("the result is"+num1/num2);
}catch (ArithmeticException e){
System.out.println("can't be divided by zero");
}
StudentInformation(int rollno,String name,int sapid,String branch)
{
System.out.println("Enter the rollno of student");
this.rollno=rollno;
System.out.println("Enter the name of student");
this.name=name;
System.out.println("Enter the sapid of student");
this.sapid=sapid;
System.out.println("Enter the branch of students");
this.branch=branch;
}
void Show()
{
System.out.println(rollno+""+name+""+sapid+""+branch);
}
}
}


